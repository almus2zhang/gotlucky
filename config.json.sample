    ],
    "static_mappings": [
        {
            "pattern": ".*\\.node66\\.example\\.com",
            "description": "Provider-A ➔ {server_name} ➔ {internal_addr}",
            "comment": "External-Gateway",
            "TLS": true
        },
        {
            "pattern": ".*\\.stun\\.ipv4\\.example\\.com",
            "description": "STUN-INTERNAL ➔ {server_name} ➔ {internal_addr}",
            "comment": "Internal-Stun",
            "undisplay": true
        },
        {
            "pattern": ".*\\.alpha\\.example\\.com",
            "description": "{server_name} ➔ {internal_addr} ➔ {stun_server_name} ➔ {stun_internal_addr}",
            "comment": "Global-Stun",
            "stun_route": {
                "\\.alpha\\.example\\.com": "\\.stun\\.v4\\.example\\.com"
            }
        },
        {
            "pattern": ".*\\.router\\.example\\.com",
            "description": "FRP-Relay ➔ {server_name} ➔ {internal_addr}",
            "comment": "Router-Link",
            "undisplay": false,
            "port_map": {
                "16666": "11466"
            }
        },
        {
            "pattern": ".*\\.relay\\.example\\.com",
            "description": "{server_name} ➔ {internal_addr}",
            "comment": "Relay-Link",
            "undisplay": false,
            "port_map": {
                "16666": "55555"
            }
        }
    ],
    "cloudflare": {
        "api_token": "YOUR_CLOUDFLARE_API_TOKEN_HERE"
    },
    "systemd_dir": "/lib/systemd/system"
}
